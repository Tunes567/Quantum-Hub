{
  "framework": "nextjs",
  "routes": [
    { 
      "src": "/api/login",
      "methods": ["POST", "OPTIONS"],
      "dest": "/pages/api/login.js"
    },
    { 
      "src": "/api/debug-supabase",
      "methods": ["GET"],
      "dest": "/pages/api/debug-supabase.js"
    },
    { 
      "src": "/api/health",
      "methods": ["GET"],
      "dest": "/api/health.py"
    },
    { 
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "dest": "/api/app.py"
    }
  ]
} 